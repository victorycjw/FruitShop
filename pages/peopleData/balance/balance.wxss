/* pages/peopleData/balance/balance.wxss */

@import "/utils/style/flexstyle.wxss";

page {
  background: #f5f5f5;
  height: 100%
}
.top_img{
  position: relative;
}

.top_img_window{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 100%;
  color: #fff;
}

.money_title{
  height: 88rpx;
  padding: 0 30rpx;
  background: #fff;
  border-bottom: 1rpx solid #ededed;
}

.money_icons{
  background: #fff;
  flex-wrap: wrap
}
.money_icons >view{
  width: 33.2%;
  border-bottom: 1rpx solid #eee;
  border-right: 1rpx solid #eee;
  text-align: center;
  padding-top: 30rpx;
  padding-bottom: 30rpx
}

.icon_title{
  color: #4d4d4d;
  margin-top: 20rpx
}


