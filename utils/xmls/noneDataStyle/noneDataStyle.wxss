@import "../../style/otherStyle/otherStyle.wxss";

.none_data_style {
  text-align: center;
  margin-top: 200rpx;
  position: absolute;
  width: 100%;
}

.none_data_text {
  margin-top: 20rpx;
  font-family: "楷体";
  font-size: 28rpx;
  color: #999;
}

.endlist {
  width: 100%;
  text-align: center;
  margin-top: 20rpx;
  font-size: 28rpx;
  color: #999;
  padding: 30rpx;
  background: #fff;
}

.nonestyle {
  opacity: 0;
}

.loading {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
  background: #fff;
}

.ac, .allcenter {
  display: flex;
  justify-content: center;
  align-items: center;
}
.vc {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
